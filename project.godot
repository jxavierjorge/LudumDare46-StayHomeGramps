; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="StayHome"
run/main_scene="res://Scenes/TitleScreen.tscn"

[autoload]

Score="*res://Scripts/Score.gd"
Soundtrack="*res://Scenes/Soundtrack.tscn"
ScoreUi="*res://Scenes/Score_UI.tscn"
UserData="*res://addons/github-integration/scripts/user_data.gd"
IconLoaderGithub="*res://addons/github-integration/scripts/IconLoaderGithub.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "github-integration" )

[rendering]

environment/default_clear_color=Color( 0.984314, 0.733333, 0.678431, 1 )
environment/default_environment="res://default_env.tres"
